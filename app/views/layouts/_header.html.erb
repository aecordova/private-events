<header>
  <nav class="nav">
  <% if logged_in? %>
    Logged in as: <%= current_user.name %>
    <%= link_to 'Log Out', logout_path, class: 'nav-link' %>
  <% else %>
    <%= link_to 'Sign Up', signup_path, class: 'nav-link' %>
    <%= link_to 'Log in', login_path, class: 'nav-link' %>
  <% end %>
  </nav>
</header>
