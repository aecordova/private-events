<div>
  <h3><%= section_title %></h3>
  <table>
    <tr>
      <th>Name</th><th>Place</th><th>Date</th>
    </tr>
    <% events.each do |e| %>
      <tr>
        <td><%= link_to e.name, e %></td>
        <td><%= e.place %></td>
        <td><%= e.date %></td>
      </tr>
    <% end %>
  </table>
</div>

