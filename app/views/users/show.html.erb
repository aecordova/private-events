
<h1>User Information:</h1>

<section>
  Name: <%= @user.name  %>
  Email: <%= @user.email %>
</section>
<section>
  <h3>Organized Events:</h3>
  <table>
    <tr>
      <th>Name</th><th>Place</th><th>Date</th>
    </tr>
    <% @user.organized_events.each do |e| %>
      <tr>
        <td><%= e.name %></td>
        <td><%= e.place %></td>
        <td><%= e.date %></td>
      </tr>
    <% end %>
  </table>

</section>
